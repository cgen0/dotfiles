! urxvt settings--------------------------------------------------------
!
! general
!
URxvt.perl-ext-common: default,matcher
URxvt.url-launcher: /usr/bin/xdg-open
URxvt.matcher.button: 1
URxvt.internalBorder: 61
URxvt.scrollBar:        false
URxvt.font:             xft:Iosevka Term:size=12:antialias=true:hinting=true, xft:dejavu sans mono:size=12:antialias=true,hinting=true
URxvt.boldFont:         xft:Iosevka Term:size=12:antialias=true:hinting=true, xft:dejavu sans mono:size=12:antialias=true,hinting=true
URxvt.italicFont:       xft:Iosevka Term:size=12:antialias=true:hinting=true, xft:dejavu sans mono:size=12:antialias=true,hinting=true
URxvt.bolditalicFont:   xft:Iosevka Term:size=12:antialias=true:hinting=true, xft:dejavu sans mono:size=12:antialias=true,hinting=true
rofi.theme: ~/.cache/wal/colors-rofi-dark.rasi

!Xft.dpi: 192

! █▓▒░ scorcerer colors
*.foreground:   #c2c2c2
*.background:   #222222
*.cursorColor:  #c2c2b0
! black
*.color0:       #333333
*.color8:       #333333
! red
*.color1:       #aa4450
*.color9:       #ff6a6a
! green
*.color2:       #719611
*.color10:      #b1d631
! yellow
*.color3:       #ff9800
*.color11:      #87875f
! blue
*.color4:       #6688aa
*.color12:      #90b0d1
! magenta
*.color5:       #8f6f8f
*.color13:      #8181a6
! cyan
*.color6:       #528b8b
*.color14:      #87ceeb
! white
*.color7:       #d3d3d3
*.color15:      #c1cdc1
! stylized
*.colorIT:      #719611
*.colorBD:      #d3d3d3
*.colorUL:      #528b8b
